{
  "name": "html-rspack-plugin",
  "version": "6.1.3",
  "license": "MIT",
  "description": "Simplifies creation of HTML files to serve your Rspack bundles",
  "author": "Jan Nicklas <j.nicklas@me.com> (https://github.com/jantimon)",
  "main": "lib/index.js",
  "types": "typings.d.ts",
  "files": [
    "lib/",
    "typings.d.ts"
  ],
  "scripts": {
    "posttest": "tsc",
    "commit": "git-cz",
    "build-examples": "node examples/build-examples.js",
    "test": "jest --runInBand --verbose --coverage",
    "test-watch": "jest --runInBand --watch",
    "puml": "npx puml generate flow.puml -o flow.png",
    "release": "standard-version"
  },
  "packageManager": "pnpm@9.12.3",
  "dependencies": {
    "@rspack/lite-tapable": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.5",
    "css-loader": "5.2.7",
    "dir-compare": "^3.3.0",
    "html-loader": "2.1.2",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "prettier": "^3.3.3",
    "pug": "3.0.3",
    "pug-loader": "2.4.0",
    "raw-loader": "4.0.2",
    "rimraf": "^4.4.1",
    "commit-and-tag-version": "^9.5.0",
    "style-loader": "2.0.0",
    "typescript": "4.9.5",
    "webpack": "^5.96.1",
    "webpack-cli": "4.10.0",
    "webpack-recompilation-simulator": "3.2.0"
  },
  "peerDependencies": {
    "@rspack/core": "0.x || 1.x"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    }
  },
  "keywords": [
    "rspack",
    "plugin",
    "html",
    "html-rspack-plugin"
  ],
  "bugs": "https://github.com/rspack-contrib/html-rspack-plugin/issues",
  "homepage": "https://github.com/rspack-contrib/html-rspack-plugin",
  "repository": "https://github.com/rspack-contrib/html-rspack-plugin.git",
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "testEnvironment": "node"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/html-webpack-plugin"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
